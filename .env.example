# Algority Environment Configuration
# Copy this to .env and .env.local

# =============================================================================
# Supabase (Auth + Database)
# =============================================================================
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-service-role-key

# =============================================================================
# OpenRouter (AI Coaching)
# =============================================================================
OPENROUTER_API_KEY=sk-or-v1-...

# =============================================================================
# Judge0 (Code Execution)
# =============================================================================
JUDGE0_API_URL=http://your-judge0-instance:2358
JUDGE0_API_KEY=your-key-if-using-rapidapi

# =============================================================================
# Upstash Redis (Caching)
# Supports both native names and Vercel KV defaults
# =============================================================================
KV_REST_API_URL=https://your-redis.upstash.io
KV_REST_API_TOKEN=your-token

# =============================================================================
# LangSmith (Tracing & Observability)
# =============================================================================
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=lsv2_pt_...
LANGSMITH_PROJECT=algority-prod
